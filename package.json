{
  "name": "custom-vscode-themes",
  "displayName": "Custom Visual Studio Code Themes",
  "description": "A collection of custom themes for vscode featuring an alternative to Monokai Dimmed, since my friends always make fun of that theme when they see it",
  "version": "1.0.0",
  "publisher": "julienpoeschl",
  "repository": {
    "type": "git",
    "url": "https://github.com/julienpoeschl/Custom-VisualStudioCode-Themes.git"
  },
  "keywords": [
    "theme",
    "themes",
    "dark",
    "dark theme",
    "Hedgehog",
    "hedgehog"
  ],
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Hedgehog (Custom)",
        "uiTheme": "vs-dark",
        "path": "./themes/Hedgehog.json"
      },
      {
        "label": "Monokai Dimmed New (Custom)",
        "uiTheme": "vs-dark",
        "path": "./themes/MonokaiDimmedNew.json"
      }
    ]
  }
}
